data x = 1 .
data y = 2 .
begin
  in x . 
  in y . 
  x = 27 .
  begin 
    out x - 1 .
    y = x / y .
    x = y * 42 .
    x = 7 + 4 .
    x = * y . 
  iffy [ x < y ] then
    begin
      out 5 . 
      x = 42 .
    end .
  y = 0 .
  iffy [ x >> y ] then
    out 42 . .
  x = y .
  iffy [ x >> y ] then
    out 100 . .
  end 
end